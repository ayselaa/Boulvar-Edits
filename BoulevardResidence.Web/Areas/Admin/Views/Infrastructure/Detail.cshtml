@model BoulevardResidence.Web.Areas.Admin.ViewModels.Infrastructure.InfrastructureDetailVM
@{
    ViewData["Title"] = "Detail";
}

<div class="sliderdetails">

<h1>Infrastructure Detail</h1>

<div>
    <h4>INFRASTRUCTURE ID: @Model.Id</h4>
</div>

<hr />

@foreach (var translate in Model.Translates)
{
    <div class="width50 mt-div">
        <h5>@translate.LangCode</h5>
        <p>Name: @translate.Name</p>
         <p>Description: @translate.Description</p>
            <hr />
    </div>

 
}

    <a class="detail-back" asp-action="Index">Back to List</a>

</div>

