                                            </div>

                                            <div class="form-group">
                                                <input type="number" name="PhoneNumber"
                                                   placeholder="+994 (000) 000 00 00" class="form-control">
                                            </div>
                                            <input type="hidden" name="ApartmentInfo" value="BLOCK @Model?.SectionName APARTMENT @Model?.Number">
