@model List<BoulevardResidence.Domain.Entity.Apartments.ApartmentFloor>;
@{
    ViewData["Title"] = "Index";
    var printedFloorNumbers = new HashSet<int>();
}
<style>
    .slide-radio {
        display: none; /* Radyo düğmesini gizle */
    }

        /* Radyo düğmesi kontrol edildiğinde arka plan rengini siyah yap */
        .slide-radio:checked + label {
            background-color: #000;
            color: #fff; /* Yazı rengini beyaz yapabilirsiniz */
        }
</style>

<!-- Main Start-->
<main>
    <!-- Header Background Start-->
    <section class="apartment-bg">
        <div class="container container-items">
            <div class="apartment-over-text">
                
                <h1>BUILDING @Model[0].Apartment?.SectionName &middot; FLOOR @Model[0]?.Floor?.FloorNumber</h1>
            </div>
            <div class="back-to-page">
                <a asp-controller="Home" asp-action="Index">
                    <svg width="21" height="16" viewBox="0 0 21 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M20 8H1M1 8L8.125 1M1 8L8.125 15" stroke="white" stroke-width="1.5" stroke-linecap="round"
                              stroke-linejoin="round" />
                    </svg>
                </a>
            </div>
        </div>
    </section>
    <!-- Header Background End-->
    <!-- Floor Start-->
    <section class="slider-floor">
        <div class="container">
            <div class="row">
                <div class="css-slider-wrapper">

                    <div class="slider-pagination">
                        @foreach (var item in ViewBag.Floors)
                        {
                            var floorNumber = item?.Floor?.Id;

                            if (printedFloorNumbers.Contains(floorNumber))
                            {
                                continue; // Eğer bu kat numarası daha önce yazdırıldıysa atla.
                            }

                            var radioId = "slider_" + floorNumber;
                            var labelClass = "page" + floorNumber;

                            var isChecked = floorNumber == ViewBag.SelectedFloor ? "checked" : "";
                            var backgroundColor = floorNumber == ViewBag.SelectedFloor ? "background-color:#181b39;" : "";
                            var color = floorNumber == ViewBag.SelectedFloor ? "color: white;" : "";


                            <a asp-controller="Floor" asp-action="Index" asp-route-sectionName="@Model[0].Apartment?.SectionName" asp-route-floornumber="@floorNumber">
                                <label for="@radioId" class="@labelClass" style="@backgroundColor @color">
                                    <span></span> 
                               
                                </label>
                            </a>



                            printedFloorNumbers.Add(floorNumber); // Bu kat numarasını işaretleyin.
                        }
                    </div>



                    <div class="sliderfloor slide@Model[0].Floor?.FloorNumber">
                        <div class="center-item-slider">
                            <!-- 2 ci MERTEBE START-->
                            <div class="project__buildings buildings-css">
                                <div class="project__img">
                                    <!-- Metebe 2-nin umumi bg image-->
                                    <img class="img-building" src="~/floorplangallery/@Model[0]?.FloorPlan" alt="img">
                                </div>
                                <div class="building">

                                    <span class="top-street a2top-street">GULBALA ALIYEV</span>
                                    <span class="bottom-street a2bottom-street">GULBALA ALIYEV</span>
                                    <span class="right-street a2right-street">GULBALA ALIYEV</span>
                                    <span class="left-street a2left-street">GULBALA ALIYEV</span>

                                    <div class="building__svgg" style="width: 100%;
                        height: 100%;">



                                        <svg style="width: 100%;" viewBox="0 0 1440 817" fill="none">
                                            <!-- 2 CI MERTEBE EV 1 BASLANGIC -->
                                            @foreach (var item in Model)


                                            {


                                                @if (item.Apartment.Status == "AVAILABLE")


                                                {


                                                    <a class="hv-white" asp-controller="Apartment" asp-action="ApartmentDetail" asp-route-id="@item.Apartment.Id">


                                                        @Html.Raw(item.Apartment.GTagPlan);


                                                    </a>


                                                }


                                                else


                                                {


                                                    @Html.Raw(item.Apartment.NotAviableGTagPlan)
                                                    ;
                                                }

                                            }

                                            }

